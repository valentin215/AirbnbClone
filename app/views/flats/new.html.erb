<p>Find me in app/views/flats/new.html.erb</p>

<h1>New flat</h1>

<%= simple_form_for [@flat] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :lat %>
  <%= f.input :long %>
  <%= f.input :capacity %>
  <%= f.input :price %>
  <%= f.input :picture %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.submit "add a Pic", class: "btn btn-primary" %>
<% end %>


